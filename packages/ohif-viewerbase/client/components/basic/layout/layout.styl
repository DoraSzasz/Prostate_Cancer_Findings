/* latin-ext */
@font-face {
  font-family: 'Roboto';
  font-style: normal;
  font-weight: 100;
  src: local('Roboto Thin'), local('Roboto-Thin'),
    url('/packages/ohif_viewerbase/assets/fonts/Roboto-Thin-latin-ext.woff2') format('woff2'),
